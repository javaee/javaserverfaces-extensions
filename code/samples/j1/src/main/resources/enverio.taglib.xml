<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
	<namespace>http://www.enverio.com/jsf</namespace>
	<tag>
  		<tag-name>serverSuggest</tag-name>
  		<handler-class>com.enverio.jsf.UISuggestHandler</handler-class>
  		<component>
    		<component-type>com.enverio.Suggest</component-type>
    		<renderer-type>com.enverio.ServerSuggest</renderer-type>
  		</component>
	</tag>
	<tag>
  		<tag-name>clientSuggest</tag-name>
  		<handler-class>com.enverio.jsf.UISuggestHandler</handler-class>
  		<component>
    		<component-type>com.enverio.Suggest</component-type>
    		<renderer-type>com.enverio.ClientSuggest</renderer-type>
  		</component>
	</tag>
	<tag>
		<tag-name>lookup</tag-name>
		<converter>
			<converter-id>com.enverio.LookupConverter</converter-id>
			<handler-class>com.enverio.jsf.LookupHandler</handler-class>
		</converter>
	</tag>
	<function>
		<function-name>selectUom</function-name>
		<function-class>com.enverio.action.OrderEntry</function-class>
		<function-signature>Object selectUom(java.util.List)</function-signature>
	</function>
</facelet-taglib>
